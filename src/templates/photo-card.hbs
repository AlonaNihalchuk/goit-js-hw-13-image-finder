{{#each this}}
  <li class='gallery-item'>
    <div class='photo-card'>
      <img src='{{webformatURL}}' alt='photo-card of {{webformatURL}}' />

      <div class='stats'>
        <p class='stats-item'>
          <span class='material-icons'>
            thumb_up
          </span>
          <i class='material-icons'>{{likes}}</i>
          1108
        </p>
        <p class='stats-item'>
          <span class='material-icons'>
            visibility
          </span>
          <i class='material-icons'>{{views}}</i>
          320321
        </p>
        <p class='stats-item'>
          <span class='material-icons'>
            comment
          </span>
          <i class='material-icons'>{{comments}}</i>
          129
        </p>
        <p class='stats-item'>
          <span class='material-icons'>
            cloud_download
          </span>
          <i class='material-icons'>{{downloads}}</i>
          176019
        </p>
      </div>
    </div>
  </li>
{{/each}}